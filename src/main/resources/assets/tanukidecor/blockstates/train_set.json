{
  "multipart": [
    {
      "apply": { "model": "tanukidecor:block/train_set/empty" }
    },
    {
      "when": { "AND": [ { "width": 0 }, { "depth": 0 }, { "facing": "north" } ] },
      "apply": { "model": "tanukidecor:block/train_set/track_corner" }
    },
    {
      "when": { "AND": [ { "width": 1 }, { "depth": 0 }, { "facing": "north" } ] },
      "apply": { "model": "tanukidecor:block/train_set/track_straight" }
    },
    {
      "when": { "AND": [ { "width": 2 }, { "depth": 0 }, { "facing": "north" } ] },
      "apply": { "model": "tanukidecor:block/train_set/track_corner", "y": 270 }
    },
    {
      "when": { "AND": [ { "width": 0 }, { "depth": 1 }, { "facing": "north" } ] },
      "apply": { "model": "tanukidecor:block/train_set/track_straight", "y": 90 }
    },
    {
      "when": { "AND": [ { "width": 2 }, { "depth": 1 }, { "facing": "north" } ] },
      "apply": { "model": "tanukidecor:block/train_set/track_straight", "y": 270 }
    },
    {
      "when": { "AND": [ { "width": 0 }, { "depth": 2 }, { "facing": "north" } ] },
      "apply": { "model": "tanukidecor:block/train_set/track_corner", "y": 90 }
    },
    {
      "when": { "AND": [ { "width": 1 }, { "depth": 2 }, { "facing": "north" } ] },
      "apply": { "model": "tanukidecor:block/train_set/track_straight", "y": 180 }
    },
    {
      "when": { "AND": [ { "width": 2 }, { "depth": 2 }, { "facing": "north" } ] },
      "apply": { "model": "tanukidecor:block/train_set/track_corner", "y": 180 }
    },

    {
      "when": { "AND": [ { "width": 0 }, { "depth": 0 }, { "facing": "east" } ] },
      "apply": { "model": "tanukidecor:block/train_set/track_corner", "y": 90 }
    },
    {
      "when": { "AND": [ { "width": 1 }, { "depth": 0 }, { "facing": "east" } ] },
      "apply": { "model": "tanukidecor:block/train_set/track_straight", "y": 90 }
    },
    {
      "when": { "AND": [ { "width": 2 }, { "depth": 0 }, { "facing": "east" } ] },
      "apply": { "model": "tanukidecor:block/train_set/track_corner" }
    },
    {
      "when": { "AND": [ { "width": 0 }, { "depth": 1 }, { "facing": "east" } ] },
      "apply": { "model": "tanukidecor:block/train_set/track_straight", "y": 180 }
    },
    {
      "when": { "AND": [ { "width": 2 }, { "depth": 1 }, { "facing": "east" } ] },
      "apply": { "model": "tanukidecor:block/train_set/track_straight" }
    },
    {
      "when": { "AND": [ { "width": 0 }, { "depth": 2 }, { "facing": "east" } ] },
      "apply": { "model": "tanukidecor:block/train_set/track_corner", "y": 180 }
    },
    {
      "when": { "AND": [ { "width": 1 }, { "depth": 2 }, { "facing": "east" } ] },
      "apply": { "model": "tanukidecor:block/train_set/track_straight", "y": 270 }
    },
    {
      "when": { "AND": [ { "width": 2 }, { "depth": 2 }, { "facing": "east" } ] },
      "apply": { "model": "tanukidecor:block/train_set/track_corner", "y": 270 }
    },

    {
      "when": { "AND": [ { "width": 0 }, { "depth": 0 }, { "facing": "south" } ] },
      "apply": { "model": "tanukidecor:block/train_set/track_corner", "y": 180 }
    },
    {
      "when": { "AND": [ { "width": 1 }, { "depth": 0 }, { "facing": "south" } ] },
      "apply": { "model": "tanukidecor:block/train_set/track_straight", "y": 180 }
    },
    {
      "when": { "AND": [ { "width": 2 }, { "depth": 0 }, { "facing": "south" } ] },
      "apply": { "model": "tanukidecor:block/train_set/track_corner", "y": 90 }
    },
    {
      "when": { "AND": [ { "width": 0 }, { "depth": 1 }, { "facing": "south" } ] },
      "apply": { "model": "tanukidecor:block/train_set/track_straight", "y": 270 }
    },
    {
      "when": { "AND": [ { "width": 2 }, { "depth": 1 }, { "facing": "south" } ] },
      "apply": { "model": "tanukidecor:block/train_set/track_straight", "y": 90 }
    },
    {
      "when": { "AND": [ { "width": 0 }, { "depth": 2 }, { "facing": "south" } ] },
      "apply": { "model": "tanukidecor:block/train_set/track_corner", "y": 270 }
    },
    {
      "when": { "AND": [ { "width": 1 }, { "depth": 2 }, { "facing": "south" } ] },
      "apply": { "model": "tanukidecor:block/train_set/track_straight" }
