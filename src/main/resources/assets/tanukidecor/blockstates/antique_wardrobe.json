{
  "multipart": [
    {
      "apply": { "model": "tanukidecor:block/antique_wardrobe/empty" }
    },
    {
      "when": { "AND": [ { "width": 1 }, { "height": 1 }, { "facing": "north" } ] },
      "apply": { "model": "tanukidecor:block/antique_wardrobe/bottom_east" }
    },
    {
      "when": { "AND": [ { "width": 2 }, { "height": 1 }, { "facing": "north" } ] },
      "apply": { "model": "tanukidecor:block/antique_wardrobe/bottom_west" }
    },
    {
      "when": { "AND": [ { "width": 1 }, { "height": 2 }, { "facing": "north" } ] },
      "apply": { "model": "tanukidecor:block/antique_wardrobe/top_east" }
    },
    {
      "when": { "AND": [ { "width": 2 }, { "height": 2 }, { "facing": "north" } ] },
      "apply": { "model": "tanukidecor:block/antique_wardrobe/top_west" }
    },
    
    {
      "when": { "AND": [ { "width": 1 }, { "height": 1 }, { "facing": "south" } ] },
      "apply": { "model": "tanukidecor:block/antique_wardrobe/bottom_east", "y": 180 }
    },
    {
      "when": { "AND": [ { "width": 2 }, { "height": 1 }, { "facing": "south" } ] },
      "apply": { "model": "tanukidecor:block/antique_wardrobe/bottom_west", "y": 180 }
    },
    {
      "when": { "AND": [ { "width": 1 }, { "height": 2 }, { "facing": "south" } ] },
      "apply": { "model": "tanukidecor:block/antique_wardrobe/top_east", "y": 180 }
    },
    {
      "when": { "AND": [ { "width": 2 }, { "height": 2 }, { "facing": "south" } ] },
      "apply": { "model": "tanukidecor:block/antique_wardrobe/top_west", "y": 180 }
    },

    {
      "when": { "AND": [ { "width": 1 }, { "height": 1 }, { "facing": "east" } ] },
      "apply": { "model": "tanukidecor:block/antique_wardrobe/bottom_east", "y": 90 }
    },
    {
      "when": { "AND": [ { "width": 2 }, { "height": 1 }, { "facing": "east" } ] },
      "apply": { "model": "tanukidecor:block/antique_wardrobe/bottom_west", "y": 90 }
    },
    {
      "when": { "AND": [ { "width": 1 }, { "height": 2 }, { "facing": "east" } ] },
      "apply": { "model": "tanukidecor:block/antique_wardrobe/top_east", "y": 90 }
    },
    {
      "when": { "AND": [ { "width": 2 }, { "height": 2 }, { "facing": "east" } ] },
      "apply": { "model": "tanukidecor:block/antique_wardrobe/top_west", "y": 90 }
    },
    
    {
      "when": { "AND": [ { "width": 1 }, { "height": 1 }, { "facing": "west" } ] },
      "apply": { "model": "tanukidecor:block/antique_wardrobe/bottom_east", "y": 270 }
    },
    {
      "when": { "AND": [ { "width": 2 }, { "height": 1 }, { "facing": "west" } ] },
      "apply": { "model": "tanukidecor:block/antique_wardrobe/bottom_west", "y": 270 }
    },
    {
      "when": { "AND": [ { "width": 1 }, { "height": 2 }, { "facing": "west" } ] },
      "apply": { "model": "tanukidecor:block/antique_wardrobe/top_east", "y": 270 }
    },
    {
      "when": { "AND": [ { "width": 2 }, { "height": 2 }, { "facing": "west" } ] },
      "apply": { "model": "tanukidecor:block/antique_wardrobe/top_west", "y": 270 }
    }
  ]
}