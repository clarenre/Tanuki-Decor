{
  "multipart": [
    {
      "when": { "AND": [ { "height": 0 }, { "facing": "north" } ] },
      "apply": { "model": "tanukidecor:block/science_pod/yellow/bottom" }
    },
    {
      "when": { "AND": [ { "height": 1 }, { "facing": "north" } ] },
      "apply": { "model": "tanukidecor:block/science_pod/yellow/middle" }
    },
    {
      "when": { "AND": [ { "height": 2 }, { "facing": "north" } ] },
      "apply": { "model": "tanukidecor:block/science_pod/yellow/top" }
    },
    {
      "when": { "AND": [ { "height": 0 }, { "facing": "south" } ] },
      "apply": { "model": "tanukidecor:block/science_pod/yellow/bottom", "y": 180 }
    },
    {
      "when": { "AND": [ { "height": 1 }, { "facing": "south" } ] },
      "apply": { "model": "tanukidecor:block/science_pod/yellow/middle", "y": 180 }
    },
    {
      "when": { "AND": [ { "height": 2 }, { "facing": "south" } ] },
      "apply": { "model": "tanukidecor:block/science_pod/yellow/top", "y": 180 }
    },
    {
      "when": { "AND": [ { "height": 0 }, { "facing": "west" } ] },
      "apply": { "model": "tanukidecor:block/science_pod/yellow/bottom", "y": 270 }
    },
    {
      "when": { "AND": [ { "height": 1 }, { "facing": "west" } ] },
      "apply": { "model": "tanukidecor:block/science_pod/yellow/middle", "y": 270 }
    },
    {
      "when": { "AND": [ { "height": 2 }, { "facing": "west" } ] },
      "apply": { "model": "tanukidecor:block/science_pod/yellow/top", "y": 270 }
    },
    {
      "when": { "AND": [ { "height": 0 }, { "facing": "east" } ] },
      "apply": { "model": "tanukidecor:block/science_pod/yellow/bottom", "y": 90 }
    },
    {
      "when": { "AND": [ { "height": 1 }, { "facing": "east" } ] },
      "apply": { "model": "tanukidecor:block/science_pod/yellow/middle", "y": 90 }
    },
    {
      "when": { "AND": [ { "height": 2 }, { "facing": "east" } ] },
      "apply": { "model": "tanukidecor:block/science_pod/yellow/top", "y": 90 }
    }
  ]
}