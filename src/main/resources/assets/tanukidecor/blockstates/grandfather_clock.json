{
  "multipart": [
    {
      "apply": { "model": "tanukidecor:block/grandfather_clock/empty" }
    },
    {
      "when": { "AND": [ { "height": 0 }, { "facing": "north" } ] },
      "apply": { "model": "tanukidecor:block/grandfather_clock/bottom" }
    },
    {
      "when": { "AND": [ { "height": 1 }, { "facing": "north" } ] },
      "apply": { "model": "tanukidecor:block/grandfather_clock/middle" }
    },
    {
      "when": { "AND": [ { "height": 2 }, { "facing": "north" } ] },
      "apply": { "model": "tanukidecor:block/grandfather_clock/top" }
    },
    {
      "when": { "AND": [ { "height": 0 }, { "facing": "south" } ] },
      "apply": { "model": "tanukidecor:block/grandfather_clock/bottom", "y": 180 }
    },
    {
      "when": { "AND": [ { "height": 1 }, { "facing": "south" } ] },
      "apply": { "model": "tanukidecor:block/grandfather_clock/middle", "y": 180 }
    },
    {
      "when": { "AND": [ { "height": 2 }, { "facing": "south" } ] },
      "apply": { "model": "tanukidecor:block/grandfather_clock/top", "y": 180 }
    },
    {
      "when": { "AND": [ { "height": 0 }, { "facing": "west" } ] },
      "apply": { "model": "tanukidecor:block/grandfather_clock/bottom", "y": 270 }
    },
    {
      "when": { "AND": [ { "height": 1 }, { "facing": "west" } ] },
      "apply": { "model": "tanukidecor:block/grandfather_clock/middle", "y": 270 }
    },
    {
      "when": { "AND": [ { "height": 2 }, { "facing": "west" } ] },
      "apply": { "model": "tanukidecor:block/grandfather_clock/top", "y": 270 }
    },
    {
      "when": { "AND": [ { "height": 0 }, { "facing": "east" } ] },
      "apply": { "model": "tanukidecor:block/grandfather_clock/bottom", "y": 90 }
    },
    {
      "when": { "AND": [ { "height": 1 }, { "facing": "east" } ] },
      "apply": { "model": "tanukidecor:block/grandfather_clock/middle", "y": 90 }
    },
    {
      "when": { "AND": [ { "height": 2 }, { "facing": "east" } ] },
      "apply": { "model": "tanukidecor:block/grandfather_clock/top", "y": 90 }
    }
  ]
}